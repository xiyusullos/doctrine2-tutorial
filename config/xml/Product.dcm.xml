<!-- config/xml/Product.dcm.xml -->
<doctrine-mapping
        xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping">
    <entity name="Product" table="products">
        <id name="id" type="integer">
            <generator strategy="AUTO" />
        </id>

        <field name="name" type="string"></field>
    </entity>
</doctrine-mapping>